page{
  background: #f7f7f7;
}
.all_title{
  height: 128rpx;
  font-size: 32rpx;
  color: #666;
  line-height: 128rpx;
  padding: 0 30rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.h4_title{
  width: 100%;
  height: 84rpx;
  padding: 0 30rpx;
  background: #fff;
}
.h4_title text{
  width: 100%;
  border-bottom:1rpx solid #eee;
  font-size: 28rpx;
  color: #666;
  line-height: 84rpx;
  display: inline-block;
  
}
.top{
  display: flex;
  padding: 50rpx 0 30rpx 30rpx;
  background: #fff;
}
.top .house{
  width: 180rpx;
  height: 125rpx;
  margin-right: 30rpx;
  background: #f7f7f7;
}
.top .h1{
  width: 120rpx;
  font-size: 28rpx;
  color: #666;
}

.top .news .tit{
  font-size: 28rpx;
  color: #666;
  display: block;
  padding-right: 30rpx;
}
.top .news .text{
  font-size: 24rpx;
  line-height: 40rpx;
  color: #999;
  display: block;
  /* padding-right: 30rpx; */
}
.tops {
  background: #fff;
  padding: 30rpx 30rpx;
  margin-top: 30rpx;

}
.tops .tit{
  font-size: 32rpx;
  color: #666;
  
}
.tops .choose {
  padding: 30rpx 0;
  display: flex;
}
.tops .choose .first{
  width: 240rpx;
	height: 60rpx;
  font-size: 28rpx;
  color: #666;
  text-align: center;
	background-color: #f7f7f7;
	border-radius: 6rpx;
	border: solid 1rpx #cccccc;
}
.tops .choose .time{
    width: 209rpx;
    height: 60rpx;
    position: relative;
}
.tops .choose .time .line{
  width: 130rpx;
  height: 25rpx;
  border-bottom: 1px solid #f54d28;
  margin-top: 8rpx;
  margin-left: 40rpx;
}
.tops .choose .time .days{
  width: 60rpx;
	height: 25rpx;
  font-size: 18rpx;
  color: #f54d28;
  text-align: center;
  line-height: 25rpx;
	background-color: #ffffff;
	border-radius: 12rpx;
	border: solid 1rpx #f54d28;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -30rpx;
  margin-top: -12rpx;
  z-index: 11;
}
.tip{
  width: 100%;
  height: 105rpx;
  font-size: 24rpx;
  color: #f54d28;
  background: #f7f7f7;
  text-align: center;
  line-height: 105rpx;
}
.news_num{
  padding: 30rpx 30rpx;
  background: #fff;
}
.news_num .detail{
  font-size: 28rpx;
  color: #666;
  padding: 20rpx 0 0 0;
}
.news_num .time{
  display: flex;
  border-bottom: 1rpx solid #ccc;
  justify-content:space-between;
}
.news_num .time .date{
  width: 80%;
  height: 100rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 100rpx;
}
.news_num .time .right{
  width: 20rpx;
  height: 30rpx;
  margin-top: 35rpx;
}
 .person{
  display: flex;
  justify-content: space-between;
  padding-top: 50rpx;
}
 .person .tit{
  flex: 1;
  font-size: 32rpx;
  color: #666;
}
 .count{
  flex: 1;
}
 .count .flex{
  display: flex;
  justify-content: center;
  align-content: flex-start;
  margin-bottom: 30rpx;
}
 .count text{
  font-size: 28rpx;
  color: #999;
  line-height: 44rpx;
}
 .count .less{
  width: 44rpx;
	height: 44rpx;
  font-size: 36rpx;
  color: #999;
  text-align: center;
  line-height: 40rpx;
	background-color: #ffffff;
	border: solid 1rpx #999999;
  border-radius: 50%;
  margin:0 48rpx 0 48rpx;
}
 .count .plus{
  width: 44rpx;
	height: 44rpx;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  background: #f54d28;
  border-radius: 50%;
  margin:0 0 0 48rpx;
  border:1rpx solid #f54d28;
}
.add{
  width: 750rpx;
  height: 120rpx;
  font-size: 32rpx;
  color: #f54d28;
  line-height: 120rpx;
  text-align: center;
  background: #fff;
  margin-top: 20rpx;

}

