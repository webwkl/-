  display: flex;
  font-size: 28rpx;
  color: #f54d28;
  line-height: 120rpx;
}
.insurance .go_add .right_red{
  width: 20rpx;
  height: 30rpx;
  margin-left: 10rpx;
  margin-top: 47rpx;
}
.use{
  width: 750rpx;
