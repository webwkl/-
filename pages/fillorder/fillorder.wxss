  background: #fff;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between; 
  padding: 0 30rpx;
}
.use .text{
  font-size: 28rpx;
  color: #666;
  line-height:120rpx;
}
switch{
  margin-top: 40rpx;
}
 .wx-switch-input{
  width: 100rpx!important;
	height: 50rpx!important;
	background: #f54d28 !important;
  border: 1rpx solid #ccc !important;
}
.wx-switch-input::before{
  width: 100rpx!important;
	height: 45rpx!important;
}
.wx-switch-input::after{
  width: 45rpx!important;
	height: 45rpx!important;
} 
.mine_integral{
  
  background: #fff;
  padding: 0 30rpx;
}
.mine_integral  .text{
  font-size: 24rpx;
  color: #999;
  display: flex;
  line-height: 48rpx;
}
.mine_integral  .text .integral{
  color: #f54d28;
}
.protocol{
  width: 750rpx;
  height: 130rpx;
  display: flex;
  padding: 0 30rpx ;
  margin-bottom: 120rpx;
}
.protocol .IDo{
  font-size: 24rpx;
  color: #999;
  line-height: 130rpx;
  display: flex;
}
.protocol .check{
  width:30rpx;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  background: #f54d28;
  margin-top: 50rpx;
  margin-right: 10rpx;
}
checkbox{
  margin-top: 36rpx;
}
checkbox .wx-checkbox-input{
     border-radius: 0; 
    height: 30rpx;
    width: 30rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
     border-radius: 0; 
