.section{
  width: 690rpx;
	height: 80rpx;
  display: flex;
  justify-content: center; 
  margin: 0 auto;
  background-color: #f7f7f7;
	border-radius: 6rpx;
}

.section .icon{
    width: 30rpx;  
    height: 31rpx;
    margin: 26rpx 0 0 21rpx;
    flex-direction: row;
    /* display: inline-flex; */
    
}
.section input{
  width: 600rpx;
  height: 80rpx;
  font-size: 28rpx; 
  color: #999;
  line-height: 80px;
  padding-left:20rpx;
  /* display: inline-flex; */
}
.sure{
  width: 750rpx;
  height: 70rpx;
  display: flex;
  flex-direction:row;
  justify-content:center;
  border-bottom:1px solid #ccc;
}
.sure .news{
  flex:1;
  height: 30rpx;
  font-size: 28rpx;
  line-height: 30rpx;
  color:#666;
  text-align: center;
  border-right:1px solid #ccc;
  margin-top: 20rpx;
  display: flex;
  justify-content:center;
}
.sure .news nanavigator{
  font-size: 28rpx;
  line-height: 30rpx;
  color:#666;
  text-align: center;
  display: inline;
}
.navigator-hover{
  background: #fff;
}
.sure .none{
  border:0;
}
.sure .news .ding{
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.sure .news .time{
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  line-height: 30rpx;
  color: #f54d28;
}
.sure .news .down{
  width: 25rpx;
  height: 25rpx;
  margin-left: 10rpx;
}
.swiper{
  width: 750rpx;
  background: #f7f7f7;
  padding: 30rpx;
  position: relative;
}
.swiper swiper .slide-image{
  width: 690rpx;

}
.swiper .explanation{
  display: flex;
  flex-flow: wrap; 
  align-content:space-around;
  padding-top: 10rpx;
}

.swiper .explanation .money{
  font-size:32rpx;
  line-height: 40rpx;
  color: #333;
  font-weight: bold; 
  padding-right: 18rpx;
}
.swiper .explanation .address{
  font-size: 24rpx;
  color: #333;
  line-height: 40rpx;
}
.swiper .explanation .score{
  font-size: 30rpx;
  color: #f54d28;
  line-height: 40rpx;
  font-weight: bold;
  flex: 1;
  text-align: right;
  font-style: italic ;
}
.swiper .explanation .legend{
  font-size: 20rpx;
  color: #f54d28;
}
.swiper .claim{
  font-size: 24rpx;
  color: #fff;
  background:rgba(0,0,0,0.5);
  border-radius: 6rpx;
  padding: 5rpx 15rpx;
  position: absolute;
  right: 50rpx;
  top: 50rpx;
  z-index: 11;
}
.fixed{
  width: 580rpx;
  height: 840rpx;
  text-align: center; 
  position: fixed;
  left: 50%;
  top: 50%;
  margin-left: -290rpx;
  margin-top: -420rpx;
  z-index: 11;
}
.fixed .fixed_img{
  width: 580rpx;
  height: 700rpx;
}
.fixed .fixed_icon{
  margin-top: 40rpx;
}
.fixed_bottom{
  width: 130rpx;
  height: 130rpx;
  position: fixed;
  right: 60rpx;
  bottom: 20rpx;
  z-index: 11;
}
.fixed_bottom .active{
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  left: 37rpx;
  top:30rpx;
  z-index: 2;
}
.fixed_bottom .active1{
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  left: 37rpx;
  top: 60rpx;
  z-index: 2;
}
.fixed_bottom .bottom_img{
  width:130rpx;
  height: 130rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}