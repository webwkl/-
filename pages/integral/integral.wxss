/* pages/integral/integral.wxss */
page{
  background: #f7f7f7;
}
.integral{
  width: 100%; 
}
.number{
  width: 100%;
  height: 340rpx;
  background: #fff;
  text-align: center;
  padding-top: 100rpx;
  margin-bottom: 25rpx;
}
.number_num{
  font-size: 60rpx;
  color: #f54d28;
  line-height: 80rpx;
}
.number text{
  font-size: 30rpx;
  color: #999;
  line-height: 50rpx;
}
.money{
  background: #fff;
  font-size: 35rpx;
  color: #666;
  line-height: 3;
  margin-bottom: 25rpx;
  padding-left: 25rpx;
}
.money_num{
  text-align: right;
  float: right;
  margin-right: 25rpx;
  color: #f54d28;
}
.money image{
  width: 30rpx;
  height: 30rpx;
  float: right;
  margin-top: 35rpx;
  margin-right: 25rpx;
}
.problem{
  color: #989999;
  font-size: 30rpx;
  width: 93%;
  margin: 0 auto;
  line-height: 1.5
}
.problem text{
  display: block;
}
